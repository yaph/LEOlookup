{
    "manifest_version": 3,
    "version": "2.0",
    "name": "LEO Wörterbuchsuche / Dictionary Lookup",
    "description": "Übersetzungen auf dict.leo.org suchen: Englisch, Französisch, Spanisch, Italienisch, Chinesisch, Russisch, Portugiesisch, Polnisch",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "options_page": "options.html",
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "host_permissions": ["<all_urls>"],
    "icons" : {
        "16" : "LEO-16.png",
        "32" : "LEO-32.png",
        "48" : "LEO-48.png",
        "128" : "LEO-128.png"
    }
}
